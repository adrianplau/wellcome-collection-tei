                    <msContents>
                        
                        <summary>A <title>Ḍākinīprayoga</title>; there is no work of this title recorded in <abbr>NCC</abbr>, vol. 8, pp. 6-7. Incomplete.</summary>
                        
                        <textLang mainLang="sa">Sanskrit</textLang>
                        
                        <msItem xml:id="B.19.l">
                            <title>Ḍākinīprayoga</title>
                            
                            <incipit> <locus>F. (1)v</locus>
                                <!-- Transcript -->
                                śrīgaṇeśāya namaḥ (||) atha ḍāk(i)nīprayogaḥ (||)
                                    ḍāk(i)nī aṣṭadhā proktā
                                    kriyā tā sā (?) prathaka prathaka (pṛthak pṛthak)
                                    sāmānyagāminī sarvā
                                    viśeṣā svalagāminī ||1
                            </incipit>
                            
                            <incipit> <locus>Ff. 2-2v</locus>
                                <!-- Transcript -->
                                ... ||4||
                                viśeṣikā durvodhvā dahati kāryaṃ dvitīghnāṃ
                                svāsadīrghikā kāmasasyaharaṃ caiva karkkasāsasyagrāhi ||
                                atha <pb n="f. 2v"/> (atha) ḍākinīprayogaḥ ||
                                mṛnmayapātram aṣṭagaṃdhena nāmākṣaraṃ liptā ...
                            </incipit>
                            
                            <explicit> <locus>F. 2v</locus>
                                <!-- Transcript -->
                                ... śatrū(n) nāmākṣara(ṃ) saha maṃtreṇa arkapatre catvāro likhitvā
                            </explicit>
                            
                            <incipit> <locus>F. 4</locus>
                                <!-- Transcript -->
                                .hastau pādau rakṣa ora kaṃ vadhvā arddharātro iti paṭhati prayogamaṃtraḥ ....
                            </incipit>
                            
                            <explicit> <locus>F. 5v</locus>
                                <!-- Transcript -->
                                ... iti bhāvaḥ oṃ namo ādeśaguruko raktakeselavāra
                            </explicit>
                            
                            <incipit> <locus>Extra leaf obverse</locus>
                                <!-- Transcript -->
                                ke vātī kāṭo amukāke kare ... kole ha ha pheḥ pheḥ pheḥ ||
                                iti ḍākinīprayogaḥ
                            </incipit>
                            
                            <explicit> <locus>Extra leaf reverse</locus>
                                <!-- Transcript -->
                                oṃ vili vili vici mi vici mī svā svāhā ||
                                prātaḥ kāle saprām imaṃ tritajā samukhaṃ prasāvyā kasyāntam asa
                                pasyo bhavati || ḍākiṇīprayogaḥ sam(a)ptā(pta)ḥ
                            </explicit>
                            
                        </msItem>
                        

                    <msContents>
                        
                        <summary>The <title>Annapūrṇādaśaka</title> ascribed to <persName role="aut">Śaṅkara</persName>
                            See <abbr>NCC</abbr>, vol. 1, p. 233. Copied by <persName role="scr">Śītalaprasāda</persName>
                            on <date>Thursday 4 February 1858</date>.</summary>
                        
                        <textLang mainLang="sa">Sanskrit</textLang>
                        
                        <msItem xml:id="Alpha_2009">
                            <title>Annapūrṇādaśaka</title>
                            <author key="person_54116823">Śaṅkara</author>
                            
                            <incipit> <locus>F. 1v</locus>
                                <!-- Transcript -->
                                maṃdārakalpaharivaṃdanapārijāta
                                madhyekṣaśāṃkamaṇimaṃḍapavedisaṃsthe |
                                ardheṃdumaulisulalāṭaṣaḍardhanetre
                                bhikṣāṃprade hi girije kṣudhitāpamahyaṃ |
                            </incipit>
                            
                            <explicit> <locus>F. 4v</locus>
                                <!-- Transcript -->
                                iti śrīmacchaṃkarācāryaviracitaṃ annapūrṇādaśakaṃ saṃpūrṇaṃ śubham astu
                                saṃvat(a) 1914 māghe vadī 8 vāra vṛhaspati liṣā śītalapraśāda
                            </explicit>
                            
                            
                        </msItem>
                        
